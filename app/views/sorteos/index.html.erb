<p id="notice"><%= notice %></p>

<h1>Listing Sorteos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sorteos.each do |sorteo| %>
      <tr>
        <td><%= link_to 'Show', sorteo %></td>
        <td><%= link_to 'Edit', edit_sorteo_path(sorteo) %></td>
        <td><%= link_to 'Destroy', sorteo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sorteo', new_sorteo_path %>
